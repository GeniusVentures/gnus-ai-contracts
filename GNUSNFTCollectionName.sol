pragma solidity ^0.8.2;

/// @title GNUSNFTCollectionName
/// @notice This contract defines the name of the Genius NFT Collection.
/// @dev This contract contains a single constant string variable that holds the name of the NFT collection.
contract GNUSNFTCollectionName {
    /// @notice The name of the Genius NFT Collection.
    /// @dev This is a constant string variable that holds the name of the NFT collection.
    string public constant name = "Genius NFT Collection";
}
